{
  "name": "shadcn-docs-nuxt",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.11.0",
  "description": "Effortless and beautiful docs template built with Nuxt Content & shadcn-vue.",
  "author": "Tony Zhang <zhangtianli2006@gmail.com>",
  "license": "MIT",
  "homepage": "https://shadcn-docs-nuxt.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZTL-UwU/shadcn-docs-nuxt.git"
  },
  "bugs": "https://github.com/ZTL-UwU/shadcn-docs-nuxt/issues",
  "keywords": [
    "markdown",
    "docs",
    "vue",
    "nuxt",
    "documentation-tool",
    "nuxt-content",
    "shadcn-vue"
  ],
  "main": "./nuxt.config.ts",
  "files": [
    "app",
    "app.vue",
    "assets",
    "components",
    "composables",
    "i18n",
    "layouts",
    "lib",
    "nuxt.config.ts",
    "pages",
    "plugins",
    "types"
  ],
  "scripts": {
    "build": "pnpm run -C www build",
    "dev": "pnpm run -C www dev",
    "generate": "pnpm run -C www generate",
    "preview": "pnpm run -C www preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "taze": "taze",
    "build:index:content": "tsx scripts/build-index-content.ts",
    "nl:build-index": "tsx scripts/build-index.ts",
    "mcp:docs": "tsx mcp-docs.ts"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.45",
    "@iconify-json/vscode-icons": "^1.2.21",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.13.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.7",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/i18n": "^9.5.4",
    "@nuxtjs/mdc": "^0.17.0",
    "@tailwindcss/vite": "^4.1.7",
    "@ztl-uwu/nuxt-content": "^2.13.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fontaine": "^0.6.0",
    "nuxt-og-image": "^5.1.4",
    "openai": "^5.23.0",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "reka-ui": "^2.3.0",
    "remark-parse": "^11.0.0",
    "remove-markdown": "^0.6.2",
    "shadcn-nuxt": "^2.2.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "tailwindcss-animate": "^1.0.7",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@iconify-json/lucide-lab": "^1.2.3",
    "@iconify/utils": "^2.3.0",
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@nuxt/content": "^3.7.1",
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/kit": "^3.17.4",
    "@nuxt/vite-builder": "^3.19.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@vueuse/core": "^13.3.0",
    "@vueuse/nuxt": "^13.3.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.27.0",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "html-to-text": "^9.0.5",
    "markdown-it": "^14.1.0",
    "minisearch": "^7.1.2",
    "node-fetch": "^3.3.2",
    "nuxt": "^3.17.4",
    "pkg-pr-new": "^0.0.51",
    "remark": "^15.0.1",
    "shiki": "^3.4.2",
    "strip-markdown": "^6.0.0",
    "taze": "^19.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}
